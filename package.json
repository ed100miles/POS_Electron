{
  "name": "mission_checker",
  "version": "1.0.0",
  "description": "",
  "main": "js/main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=false --out=release-builds",
    "package-win": "electron-packager . mission_checker --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/windows/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Mission Checker\"",
    "package-linux": "electron-packager . mission_checker --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/linux/icon.png --prune=true --out=release-builds"
  },
  "author": "Ed",
  "license": "MIT",
  "dependencies": {
    "electron": "^15.1.2",
    "python-shell": "^3.0.0"
  },
  "devDependencies": {
    "electron-packager": "^15.4.0"
  }
}
